@using DemoUpSchoolProject.Models.Entities
@model List<TblServicesFeature>
<section class="w3l-homeblock2" id="skills">
    <div class="midd-w3 py-5">
        <div class="container py-lg-5 py-md-3">
            <div class="row">
                <div class="col-lg-6 align-self">
                    <div class="position-relative px-lg-5 mx-lg-0 mx-md-5 image-padding">
                        <img src="assets/images/skills.png" alt="" class="radius-image img-fluid">
                        <div class="position-absolute-images">
                            <img src="~/Templates/iportfolio-starter_Free11-08-2020_2100992022/iportfolio-starter/assets/images/file.png" class="img-fluid pos-img" alt="" />
                            <img src="~/Templates/iportfolio-starter_Free11-08-2020_2100992022/iportfolio-starter/assets/images/file1.png" class="img-fluid pos-img1" alt="" />
                            <img src="~/Templates/iportfolio-starter_Free11-08-2020_2100992022/iportfolio-starter/assets/images/file2.png" class="img-fluid pos-img2" alt="" />
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 mt-lg-0 mt-md-5 mt-4">
                    <span class="text"></span>
                   
                    <p class="mt-4">
                        Aşağıda, öğrencilik sürecim boyunca katıldığım eğitimlerin bilgisi yer almaktadır
                    </p>
                    <div class="skills-bars mt-5">
                        @foreach (var item in Model)
                        {
                            <div class="progress-info">
                                <h6 class="progress-tittle">@item.Title - @item.Description</h6>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-striped" role="progressbar" style="width: 80%" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100">
                                    </div>
                                </div>
                            </div>

                        }



                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
