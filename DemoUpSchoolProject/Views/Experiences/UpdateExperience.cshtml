@model  DemoUpSchoolProject.Models.Entities.TblExperiences
@{
    ViewBag.Title = "UpdateExperience";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}

<h2>Deneyimi Güncelle</h2>
<form method="post">

    @Html.HiddenFor(x => x.ExperienceID, new { @class = "form-control" })

    <br />
    <label>Çalıştığı Ptofesyonellik Alanı</label>
    @Html.TextBoxFor(x => x.JobTitle, new { @class = "form-control" })


    <br />
    <label>Çalıştığı Şirket</label>
    @Html.TextBoxFor(x => x.CompanyName, new { @class = "form-control" })
    <br />
    <label>Çalıştığı Süresi</label>
    @Html.TextBoxFor(x => x.Duration, new { @class = "form-control" })
    <br />
    <label>Deneyim Açıklaması</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>




</form>

